<Window x:Class="AcademyHttpClientGUI.Instructors.SubWindows.Display"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AcademyHttpClientGUI.Instructors.SubWindows"
        mc:Ignorable="d"
        Title="Display" Width="400">
    <Grid>
        <StackPanel x:Name="Container">
            <TextBlock Text="Instructor list" FontSize="15" HorizontalAlignment="Center" Margin="0, 20, 0, 0"/>
            <ScrollViewer Margin="0, 20, 0, 0">
                <TextBlock x:Name="DisplayBlock" TextWrapping="Wrap" HorizontalAlignment="Center"/>
            </ScrollViewer>
        </StackPanel>
    </Grid>
</Window>
